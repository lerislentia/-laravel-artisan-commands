{
    "name": "lerislentia/laravel-artisan-commands",
    "description": "Some useful Artisan commands for Laravel.",
    "version": "1.0",
    "keywords": [
        "framework",
        "laravel",
        "Artisan",
        "Console",
        "Commands"
    ],
    "license": "MIT",
    "homepage": "https://github.com/lerislentia/laravel-artisan-commands",
    "authors": [
        {
            "name": "marco benvenuto",
            "email": "marco.benvenuto@hostmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=7.2.0",
        "laravel/framework": "5.8.*"
    },
	"require-dev": {
        "phpunit/phpunit": "^8.3",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "config": {
        "sort-packages": true
    },
	"autoload": {
        "psr-4": {
            "Lerislentia\\ArtisanCommands\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
